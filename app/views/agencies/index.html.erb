<h2>Agency Listing</h2>
<div class="well main">
  <% if @agencies.blank? %>
    <span class = "text-danger">No agencies found!</span>
  <% else %>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Title</th>
        </tr>
      </thead>
      <tbody>
        <%= will_paginate @agencies %>
        <% @agencies.each do |a| %>
          <tr>
            <td>
              <%= link_to(a.title, agency_properties_path(a)) %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= will_paginate @agencies %>
  <% end %>
</div>
